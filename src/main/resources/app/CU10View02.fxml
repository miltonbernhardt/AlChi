<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane stylesheets="@styles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.CU10Controller02">
   <children>
      <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" spacing="15.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="NEVER">
               <children>
                  <VBox alignment="CENTER" spacing="15.0">
                     <children>
                        <HBox alignment="TOP_CENTER" spacing="50.0">
                           <children>
                              <VBox spacing="15.0">
                                 <children>
                                    <HBox alignment="CENTER_LEFT" spacing="5.0">
                                       <children>
                                          <Label text="Producto:" />
                                          <TextField fx:id="tipoProducto" disable="true" />
                                       </children>
                                    </HBox>
                                 </children>
                              </VBox>
                              <VBox alignment="TOP_CENTER" spacing="15.0">
                                 <children>
                                    <HBox alignment="CENTER_LEFT" spacing="5.0">
                                       <children>
                                          <Label text="Paquete inicial primario:" />
                                          <TextField fx:id="productoInicialPrimario" disable="true" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" HBox.hgrow="SOMETIMES" />
                                       </children>
                                    </HBox>
                                    <Pane maxHeight="-Infinity" minHeight="-Infinity" prefHeight="20.0" />
                                    <HBox alignment="CENTER_LEFT" spacing="5.0">
                                       <children>
                                          <Label minWidth="-Infinity" text="Paquete inicial secundario:" />
                                          <ComboBox fx:id="productoInicial" minWidth="700.0" onAction="#seleccionarProducto" HBox.hgrow="SOMETIMES">
                                             <cursor>
                                                <Cursor fx:constant="HAND" />
                                             </cursor>
                                          </ComboBox>
                                       </children>
                                    </HBox>
                                    <HBox alignment="CENTER_LEFT" spacing="5.0">
                                       <children>
                                          <Label minWidth="-Infinity" text="Forma de venta:" />
                                          <TextField fx:id="formaVenta" disable="true" />
                                       </children>
                                    </HBox>
                                    <HBox alignment="CENTER_LEFT" spacing="5.0">
                                       <children>
                                          <Label text="Cantidad no vendida restante:" />
                                          <TextField fx:id="cantidadNoVendidaRestante" disable="true" />
                                       </children>
                                    </HBox>
                                 </children>
                              </VBox>
                              <VBox spacing="17.0">
                                 <children>
                                    <HBox alignment="CENTER_LEFT" spacing="5.0">
                                       <children>
                                          <Label text="Cantidad:" />
                                          <TextField fx:id="cantidadPrimaria" disable="true" />
                                       </children>
                                    </HBox>
                                    <Pane maxHeight="-Infinity" minHeight="-Infinity" prefHeight="15.0" />
                                    <HBox alignment="CENTER_LEFT" spacing="5.0">
                                       <children>
                                          <Label text="Cantidad:" />
                                          <TextField fx:id="cantidadSecundaria" disable="true" />
                                       </children>
                                    </HBox>
                                 </children>
                              </VBox>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
            <Pane maxHeight="-Infinity" minHeight="-Infinity" prefHeight="20.0" />
            <HBox alignment="CENTER" spacing="50.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#confirmar" text="Confirmar">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
                  <Button cancelButton="true" mnemonicParsing="false" onAction="#cancelar" text="Cancelar">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets left="15.0" right="15.0" top="15.0" />
   </padding>
</AnchorPane>
